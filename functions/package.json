{
  "name": "nous-toutes",
  "scripts": {
    "lint": "xo \"src/**/*\"",
    "build": "tsc",
    "watch": "tsc -w",
    "env:test": "firebase use nous-toutes-test",
    "env:prod": "firebase use nous-toutes",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest  ./src/seed --runInBand",
    "test:watch": "jest  ./src --watch --runInBand"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.22",
    "@mailchimp/mailchimp_transactional": "^1.0.19",
    "@types/papaparse": "^5.2.4",
    "axios": "^0.20.0",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "limax": "^2.1.0",
    "papaparse": "^5.3.0",
    "typesaurus": "^6.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint-config-xo": "^0.32.1",
    "eslint-config-xo-typescript": "^0.33.0",
    "firebase-functions-test": "^0.2.2",
    "typescript": "^4.0.3",
    "xo": "^0.33.1",
    "jest": "^26.5.2",
    "ts-jest": "^26.4.1"
  },
  "xo": {
    "env": [
      "node",
      "jest"
    ],
    "extends": [
      "xo",
      "xo-typescript"
    ],
    "rules": {
      "@typescript-eslint/prefer-ts-expect-error": "off"
    }
  },
  "private": true
}
